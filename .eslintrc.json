{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "plugins": [
        "prettier"
    ],
    "extends": [
        "standard",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
